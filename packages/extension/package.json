{
  "name": "svg-preview",
  "displayName": "Svg Preview",
  "description": "Preview for Svg files",
  "version": "0.0.0-semantically-released",
  "publisher": "SimonSiefke",
  "license": "MIT",
  "enableProposedApi": true,
  "homepage": "https://github.com/SimonSiefke/vscode-svg-preview/blob/master",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonSiefke/vscode-svg-preview.git"
  },
  "bugs": {
    "url": "https://github.com/SimonSiefke/vscode-svg-preview/issues"
  },
  "badges": [
    {
      "url": "https://travis-ci.org/Microsoft/vscode-python.svg?branch=master",
      "href": "https://travis-ci.org/Microsoft/vscode-python",
      "description": "Continuous integration (Travis)"
    }
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.31.0"
  },
  "appInsightsKey": "TODO",
  "categories": [
    "Other",
    "Debuggers"
  ],
  "keywords": [
    "svg preview"
  ],
  "activationEvents": [
    "onLanguage:xml",
    "onWebviewPanel:svgPreview",
    "onCommand:svgPreview.showPreview",
    "onCommand:svgPreview.showPreviewToSide"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Svg Preview",
        "command": "svgPreview.showPreview",
        "title": "%svgPreview.command.showPreview%"
      },
      {
        "category": "Svg Preview",
        "command": "svgPreview.showPreviewToSide",
        "title": "%svgPreview.command.showPreviewToSide%"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "svgPreview.showPreview",
          "when": "editorLangId == xml && resourceExtname == .svg",
          "group": "navigation"
        },
        {
          "command": "svgPreview.showPreviewToSide",
          "when": "editorLangId == xml && resourceExtname == .svg",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Svg Preview",
      "properties": {
        "svgPreview.autoOpen": {
          "type": "boolean",
          "default": false,
          "description": "%svgPreview.configuration.autoOpen%",
          "scope": "resource"
        }
      }
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "test": "echo test"
  },
  "dependencies": {
    "memoize-one": "^5.0.4"
  },
  "devDependencies": {
    "@types/node": "^11.13.2",
    "vscode": "^1.1.33"
  }
}
