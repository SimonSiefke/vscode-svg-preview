      "href": "https://codecov.io/gh/Microsoft/vscode-python",
      "description": "Test coverage"
    }
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.31.0"
  },
  "appInsightsKey": "TODO",
  "categories": [
    "Other",
    "Debuggers"
  ],
  "keywords": [
    "html preview",
    "live preview",
    "open in browser",
    "multi-root ready"
  ],
  "activationEvents": [
    "*",
    "onLanguage:xml",
    "onWebviewPanel:svgPreview",
    "onCommand:svgPreview.open"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Svg Preview",
        "command": "svgPreview.open",
        "title": "SVG: Open Preview to the Side"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "svgPreview.open",
          "when": "editorLangId == xml && resourceExtname == .svg",
          "group": "navigation"
        }
      ]
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "test": "echo test"
  },
  "dependencies": {
    "memoize-one": "^5.0.2",
    "p-cancelable": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^11.13.0",
    "vscode": "^1.1.33"
  }
}
