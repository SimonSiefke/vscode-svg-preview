{
  "name": "svg-preview",
  "displayName": "Svg Preview",
  "description": "Preview for Svg files",
  "version": "0.0.0-semantically-released",
  "publisher": "SimonSiefke",
  "license": "MIT",
  "enableProposedApi": true,
  "homepage": "https://github.com/SimonSiefke/vscode-svg-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonSiefke/vscode-svg-preview.git"
  },
  "bugs": {
    "url": "https://github.com/SimonSiefke/vscode-svg-preview/issues"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#242b2b",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "svg preview",
    "svg",
    "preview",
    "svg viewer"
  ],
  "activationEvents": [
    "onWebviewPanel:svgPreview",
    "onCommand:svgPreview.showPreview",
    "onCommand:svgPreview.showPreviewToSide"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Svg Preview",
        "command": "svgPreview.showPreview",
        "title": "Open Preview"
      },
      {
        "category": "Svg Preview",
        "command": "svgPreview.showPreviewToSide",
        "title": "Open Preview to the Side",
        "icon": {
          "light": "images/bolt_original_darkgray_optimized.svg",
          "dark": "images/bolt_original_lightgray_optimized.svg"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "svgPreview.showPreview",
          "when": "editorLangId == xml && resourceExtname == .svg",
          "group": "navigation"
        },
        {
          "command": "svgPreview.showPreviewToSide",
          "when": "editorLangId == xml && resourceExtname == .svg",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "svgPreview.showPreview",
          "group": "navigation",
          "when": "resourceLangId == xml && resourceExtname == .svg"
        }
      ],
      "editor/title": [
        {
          "command": "svgPreview.showPreviewToSide",
          "group": "navigation",
          "when": "editorLangId == xml && resourceExtname == .svg && svgPreviewIsOpen == false"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Svg Preview",
      "properties": {
        "svgPreview.autoOpen": {
          "type": "boolean",
          "default": false,
          "description": "Automatically open the preview when an svg file is opened.",
          "scope": "resource"
        },
        "svgPreview.background": {
          "type": "string",
          "default": "transparent",
          "enum": [
            "transparent",
            "checkerboard"
          ],
          "description": "The background of the preview.",
          "scope": "resource"
        }
      }
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "test": "echo test"
  },
  "dependencies": {
    "memoize-one": "^5.0.4",
    "mime-types": "^2.1.22"
  },
  "devDependencies": {
    "@types/memoize-one": "^4.1.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^11.13.4",
    "vscode": "^1.1.33"
  }
}
