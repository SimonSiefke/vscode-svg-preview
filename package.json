{
  "name": "vscode-svg-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonSiefke/vscode-svg-preview.git"
  },
  "scripts": {
    "dev": "rollup --config --no-esModule --no-strict --watch --environment NODE_ENV:development",
    "build": "rollup --config --no-esModule --no-strict --environment NODE_ENV:production",
    "lint": "eslint . && prettier **/*.* --list-different",
    "lint:fix": "eslint --fix . && prettier --write **/*.*",
    "bundlesize": "bundlesize",
    "postinstall": "lerna bootstrap",
    "package": "rm -rf dist && npm run lint && npm run build && npm run bundlesize && node ./scripts/package.js && cd dist && npx vsce package && cd .."
  },
  "keywords": [],
  "author": "Simon Siefke",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "bundlesize": "^0.17.1",
    "eslint": "^5.16.0",
    "eslint-config-bitworkers": "^1.21.10",
    "eslint-plugin-cypress": "^2.2.1",
    "fs-extra": "^7.0.1",
    "lerna": "^3.13.3",
    "prettier": "^1.17.0",
    "prettier-config-bitworkers": "^1.5.0",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-import-alias": "^1.0.6",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "typescript": "^3.4.4"
  },
  "bundlesize": [
    {
      "path": "./packages/extension/dist/extension.js",
      "maxSize": "8.7 kB",
      "compression": "none"
    },
    {
      "path": "./packages/preview/dist/index.js",
      "maxSize": "1.25 kB"
    }
  ]
}
